{
  "id": "bpmn-process-def",
  "formKey": "",
  "title": {
    "en": "BPMN Process definition form"
  },
  "description": {},
  "customModule": "",
  "typeRef": "",
  "width": "lg",
  "system": true,
  "i18n": {},
  "definition": {
    "components": [
      {
        "columns": [
          {
            "components": [
              {
                "label": {
                  "ru": "Идентификатор",
                  "en": "Identifier"
                },
                "key": "processDefId",
                "validate": {
                  "required": true,
                  "pattern": "([a-zA-Z0-9-_]+$)"
                },
                "defaultValue": "",
                "refreshOn": [
                  "lastCreatedType"
                ],
                "customDefaultValue": "let typeId = _.get(data, 'lastCreatedType.typeLocalIdInWs', '');\n\nif (typeId) {\n  value = typeId + \"-bpmn\";\n} else {\n  value = null;\n}",
                "optionalWhenDisabled": false,
                "type": "textfield",
                "input": true,
                "inputMasks": [
                  {}
                ],
                "reorder": false,
                "dataSrc": ""
              },
              {
                "label": {
                  "ru": "Тип данных",
                  "en": "Data type"
                },
                "key": "ecosType",
                "hideEditRowButton": true,
                "modalTitle": "",
                "customActionRefs": [
                  "uiserv/action@edit-ecos-type"
                ],
                "journalId": "ecos-types",
                "queryData": "",
                "sortAscending": "asc",
                "refreshOn": [
                  "lastCreatedType"
                ],
                "customDefaultValue": "let typeId = _.get(data, 'lastCreatedType.typeId', '');\n\nif (typeId) {\n  value = \"emodel/type@\" + typeId;\n} else {\n  value = null;\n}",
                "optionalWhenDisabled": false,
                "type": "selectJournal",
                "input": true
              },
              {
                "label": {
                  "ru": "Включен",
                  "en": "Enabled"
                },
                "key": "enabled",
                "refreshOn": [],
                "optionalWhenDisabled": false,
                "type": "checkbox",
                "input": true
              },
              {
                "label": "lastCreatedType",
                "source": {
                  "type": "recordsQuery",
                  "recordsQuery": {
                    "query": "value = {\n  \"sourceId\": \"emodel/types-repo\",\n  \"language\": \"predicate\",\n  \"query\": {\n    \"t\": \"and\",\n    \"v\": [\n      {\n        \"t\": \"gt\",\n        \"a\": \"_created\",\n        \"v\": (new Date(new Date() - 1000 * 60 * 10))\n      },\n      {\n        \"t\": \"eq\",\n        \"a\": \"_creator\",\n        \"v\": Citeck.constants.USERNAME\n      }\n    ]\n  },\n  \"sortBy\": [\n    {\"attribute\": \"_created\", \"ascending\": false}\n  ],\n  \"workspaces\": [Citeck.Navigator.getWorkspaceId()]\n}",
                    "attributes": {
                      "typeId": "?localId",
                      "disp": "_disp._as.mltext?json",
                      "typeLocalIdInWs": "id"
                    },
                    "isSingle": true
                  },
                  "forceLoad": false
                },
                "update": {
                  "type": "once"
                },
                "key": "lastCreatedType",
                "type": "asyncData",
                "input": true,
                "triggerEventOnChange": false
              }
            ],
            "type": "column",
            "input": false,
            "key": "column",
            "label": "Column",
            "index": 0,
            "clearOnHide": true,
            "conditional": {
              "show": null,
              "when": null
            }
          },
          {
            "index": 1,
            "type": "column",
            "input": false,
            "key": "column",
            "label": "Column",
            "components": [
              {
                "label": {
                  "ru": "Имя",
                  "en": "Name"
                },
                "key": "name",
                "refreshOn": [
                  "lastCreatedType"
                ],
                "customDefaultValue": "let typeDisp = _.get(data, 'lastCreatedType.disp', '');\n\nif (!_.isEmpty(typeDisp)) {\n  if (!typeDisp.ru && Citeck.helpers.getCurrentLocale().indexOf('ru') !== -1) {\n    typeDisp['ru'] = _.values(typeDisp)[0];\n  }\n  let valueRes = {};\n  for (let locale in typeDisp) {\n    if (locale === 'ru') {\n      valueRes[locale] = \"Процесс для '\" + typeDisp[locale] + \"'\"\n    } else {\n      valueRes[locale] = \"Process for '\" + typeDisp[locale] + \"'\"\n    }\n  }\n  value = valueRes;\n} else {\n  value = null;\n}",
                "optionalWhenDisabled": false,
                "type": "mlText",
                "input": true
              },
              {
                "label": {
                  "ru": "Раздел",
                  "en": "Section"
                },
                "key": "sectionRef",
                "validate": {
                  "select": false
                },
                "dataSrc": "custom",
                "data": {
                  "custom": "const records = _.get(data, 'sectionsList.records', []);\nconst currentSection = _.get(data, 'sectionRef', \"\");\nvalues = records.filter((category) => category.canCreate || category.id === currentSection);",
                  "headers": [
                    {}
                  ]
                },
                "refreshOn": [
                  "sectionsList"
                ],
                "customDefaultValue": "value = _.get(data, 'sectionsList.records', [])[0].id || null;",
                "optionalWhenDisabled": false,
                "customConditional": "const sections = _.get(data, 'sectionsList.records', []);\nshow = sections.length > 1 || sections[0].value !== 'eproc/bpmn-section@DEFAULT';",
                "defaultValue": "",
                "type": "ecosSelect",
                "input": true,
                "reorder": false,
                "reference": false
              },
              {
                "label": {
                  "ru": "Форма",
                  "en": "Form"
                },
                "key": "formRef",
                "modalTitle": "",
                "journalId": "ecos-forms",
                "refreshOn": [],
                "optionalWhenDisabled": false,
                "type": "selectJournal",
                "input": true,
                "queryData": ""
              },
              {
                "label": {
                  "ru": "Автоматический старт процесса",
                  "en": "Auto start process"
                },
                "key": "autoStartEnabled",
                "tooltip": {
                  "ru": "При создании объекта указанного типа, процесс будет запущен автоматически",
                  "en": "When creating an object of the specified type, the process will be launched automatically"
                },
                "refreshOn": [],
                "optionalWhenDisabled": false,
                "type": "checkbox",
                "input": true
              },
              {
                "label": {
                  "ru": "Автоматическое удаление процесса",
                  "en": "Auto delete process"
                },
                "key": "autoDeleteEnabled",
                "tooltip": {
                  "ru": "При удалении объекта указанного типа, процесс будет удален автоматически",
                  "en": "When deleting an object of the specified type, the process will be deleted automatically"
                },
                "defaultValue": true,
                "refreshOn": [],
                "optionalWhenDisabled": false,
                "type": "checkbox",
                "input": true
              }
            ],
            "clearOnHide": true,
            "conditional": {
              "show": null,
              "when": null
            }
          }
        ],
        "key": "columns3",
        "type": "columns",
        "input": false
      },
      {
        "columns": [
          {
            "components": [],
            "type": "column",
            "input": false,
            "key": "column",
            "label": "Column",
            "index": 0,
            "clearOnHide": true,
            "conditional": {
              "show": null,
              "when": null
            }
          },
          {
            "index": 1,
            "type": "column",
            "input": false,
            "key": "column",
            "label": "Column",
            "components": [
              {
                "label": {
                  "ru": "Создать",
                  "en": "Submit"
                },
                "key": "submit",
                "state": "",
                "headers": [
                  {
                    "header": "",
                    "value": ""
                  }
                ],
                "theme": "primary",
                "block": true,
                "disableOnInvalid": true,
                "refreshOn": [],
                "optionalWhenDisabled": false,
                "type": "button",
                "input": true
              }
            ],
            "clearOnHide": true,
            "conditional": {
              "show": null,
              "when": null
            }
          }
        ],
        "key": "columns2",
        "type": "columns",
        "input": false
      },
      {
        "label": "sectionsList",
        "source": {
          "type": "custom",
          "custom": {
            "syncData": "value = {};",
            "asyncData": "const currentWs = Citeck.Navigator.getWorkspaceId();\nif (currentWs && currentWs != 'default' && currentWs.indexOf('admin$') !== 0) {\n  value = Records.get('eproc/bpmn-section@DEFAULT').load({\n    'label': '?disp',\n    'value': '?id'\n  }).then(atts => {\n    atts[\"canCreate\"] = true;\n    return { records: [ atts ] };\n  });\n} else {\n  value = Records.query({\n    'sourceId': 'eproc/bpmn-section',\n    'language': 'predicate',\n    'query': { t: 'not-eq', a: 'id', v: 'ROOT' }\n  }, {\n    'label': '?disp',\n    'value': '?id',\n    'canCreate': 'permissions._has.bpmn-section-create-process-def?bool'\n  }).then(qres => {\n    return {\n      records: qres.records.filter(rec => !!rec.canCreate)\n    };\n  });\n}"
          },
          "forceLoad": false
        },
        "update": {
          "type": "once"
        },
        "key": "sectionsList",
        "type": "asyncData",
        "input": true,
        "triggerEventOnChange": false
      }
    ],
    "formId": "bpmn-process-def"
  },
  "attributes": {},
  "notExists": false
}