            "validate": {
              "required": true
            },
            "dataSrc": "values",
            "data": {
              "values": [
                {
                  "label": "decision_binding_deployment",
                  "value": "DEPLOYMENT"
                },
                {
                  "label": "decision_binding_latest",
                  "value": "LATEST"
                },
                {
                  "label": "decision_binding_version",
                  "value": "VERSION"
                },
                {
                  "label": "decision_binding_versionTag",
                  "value": "VERSION_TAG"
                }
