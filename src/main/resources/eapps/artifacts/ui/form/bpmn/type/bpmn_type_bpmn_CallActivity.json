                "validate": {
                  "required": true
                },
                "dataSrc": "values",
                "data": {
                  "values": [
                    {
                      "label": "process_binding_deployment",
                      "value": "DEPLOYMENT"
                    },
                    {
                      "label": "process_binding_latest",
                      "value": "LATEST"
                    },
                    {
                      "label": "process_binding_version",
                      "value": "VERSION"
                    },
                    {
                      "label": "process_binding_versionTag",
                      "value": "VERSION_TAG"
                    }
                  ],
                  "headers": [
                    {
                      "key": "",
                      "value": ""
                    }
                  ]
                },
                "defaultValue": "LATEST",
                "optionalWhenDisabled": false,
                "type": "ecosSelect",
                "input": true
              },
              {
                "label": {
                  "ru": "Версия",
                  "en": "Version"
